{
  "builders": [
    {
      "boot_command": [
        "<esc><wait>",
        "install <wait>",
        " auto=true",
        " priority=critical",
        " url=http://{{ .HTTPIP }}:{{ .HTTPPort }}/debian/preseed.cfg <wait>",
        "<enter>"
      ],
      "boot_wait": "5s",
      "cpus": "2",
      "disk_size": "10000",
      "guest_os_type": "Ubuntu_64",
      "headless": false,
      "http_directory": "http",
      "iso_checksum":"2af8f43d4a7ab852151a7f630ba596572213e17d3579400b5648eba4cc974ed0",
      "iso_url": "https://cdimage.debian.org/debian-cd/current/amd64/iso-cd/debian-10.6.0-amd64-netinst.iso",
      "memory": "1024",
      "output_directory": "output-debian-{{ build_type }}-{{ timestamp }}",
      "shutdown_command": "sudo systemctl poweroff",
      "ssh_handshake_attempts": "20",
      "ssh_password": "vagrant",
      "ssh_timeout": "15m",
      "ssh_username": "vagrant",
      "type": "virtualbox-iso",
      "vm_name": "packer-debian-buster-amd64"
    }],
  "provisioners": [{
    "type": "shell",
    "scripts": [
      "scripts/hello.sh"
    ]
  }],
  "post-processors": [{
    "type": "vagrant",
    "compression_level": "{{user `compression_level`}}",
    "output": "debian-buster-amd64-{{.Provider}}.box"
  }],
  "variables": {
    "compression_level": "9"
  }
}
